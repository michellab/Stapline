# Run a fit to energies using the parameters defined earlier
RUNTYPE=FIT
COORDINATE_FORMAT=TRAJECTORY
NSTRUCTURES=$NSTRUCT
K=$K 
PARAMETERS_TO_FIT=LOAD
PARAMETER_FILE_NAME=parameter_$PREFIX.in
FUNC_TO_FIT=SUM_SQUARES_AMBER_STANDARD
QM_ENERGY_UNITS=HARTREE

# Use the genetic algorithm with the following settings
ALGORITHM=$ALGORITHM
OPTIMIZATIONS=$OPT
MAX_GENERATIONS=$MAX_GEN
GENERATIONS_TO_CONV=$GEN_CONV
GENERATIONS_TO_SIMPLEX=$GEN_SIMPLEX
GENERATIONS_WITHOUT_SIMPLEX=$GEN_WT_SIMPLEX
MUTATION_RATE=$MUT_RATE
PARENT_PERCENT=$PARENT
SEARCH_SPACE=$SSPACE
SORT_MDCRDS=OFF

# Output files for later analysis

WRITE_FRCMOD=$PREFIXparamfit.frcmod
